#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules/jscodeshift/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules/jscodeshift/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules/jscodeshift/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules/jscodeshift/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/jscodeshift@0.15.2_@babel+p_4e1cf7334ac93804887c464f7351130e/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../bin/jscodeshift.js" "$@"
fi
