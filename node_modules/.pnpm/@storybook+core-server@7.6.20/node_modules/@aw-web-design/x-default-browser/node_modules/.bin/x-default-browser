#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/x-default-browser/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/x-default-browser/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/x-default-browser/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/x-default-browser/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules/@aw-web-design/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.126/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/x-default-browser.js" "$@"
else
  exec node  "$basedir/../../bin/x-default-browser.js" "$@"
fi
