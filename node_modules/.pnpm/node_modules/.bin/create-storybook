#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/dist/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/dist/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/dist/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/dist/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules/create-storybook/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/create-storybook@10.0.6_@te_760d6889ea9e4fb1d1a0b1b94cc18f3d/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-storybook/dist/bin/index.js" "$@"
else
  exec node  "$basedir/../create-storybook/dist/bin/index.js" "$@"
fi
