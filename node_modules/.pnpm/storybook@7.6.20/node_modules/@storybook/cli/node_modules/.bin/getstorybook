#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/cli/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/cli/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/cli/bin/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/cli/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules/@storybook/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/@storybook+cli@7.6.20/node_modules:/mnt/d/SM web/STEAMachines.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
